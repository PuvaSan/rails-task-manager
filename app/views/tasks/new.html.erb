<div class="container">
  <h1>New Task</h1>
    <%= form_with task: @task do |f|%>
      <%= f.label :title %>
      <%= f.text_field :title %>
      <%= f.label :details %>
      <%= f.text_field :details %>
      <%= f.submit "create task" %>
    <% end %>
</div>

<br>
<%= link_to "Back to list", tasks_path %>
